<table width="100%" border="0" align="center" style="position:absolute;top:30%;">
  <tr>
    <td align="right"></td>
    <td align="center" background="<%=asset_path("PixelBlanco.png")%>" style="background-repeat:repeat;text-align:left">
		<%= render 'nav' %>
    <%= link_to 'Back', new_participant_path %>
		<% @ganadors.each do |ganador| %>
  		<% @g = ganador.getGanadorData %>
  		<div>
    		<p><b>Nombre:</b><%= @g.nombre %></p>
    		<span><%= link_to 'Show', ganador %></span>
    		<% if @current_user %>
    			<span>|<%= link_to 'Edit', edit_ganador_path(ganador) %></span>
    			<span>|<%= link_to 'Destroy', ganador, confirm: 'Are you sure?', method: :delete %></span>
				<% end %>
  		</div>
		<% end %>
    </td>
    <td align="center">&nbsp;</td>
  </tr>
</table>
