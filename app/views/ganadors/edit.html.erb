<h1>Editing ganador</h1>

<%= form_for @ganador, :url => ganador_path(params[:id]), :html => { :multipart => true } do |f| %>
	<div class="field">
		<%= label_tag :foto %>
		<%= file_field_tag :foto %> 
	</div>
	<div class="actions"><%= submit_tag "Subir foto" %></div>
<% end %>

<%= link_to 'Show', @ganador %> |
<%= link_to 'Back', ganadors_path %>
